{
  "name": "elyssio-monorepo",
  "version": "1.0.0",
  "description": "Monorepo Elyssio - Admin & Client",
  "private": true,
  "scripts": {
    "dev:admin": "cd admin && pnpm install && pnpm dev",
    "dev:client": "cd client && pnpm install && pnpm dev",
    "build:admin": "cd admin && pnpm run build",
    "build:client": "cd client && pnpm run build",
    "start:admin": "cd admin && pnpm run start",
    "start:client": "cd client && pnpm run start",
    "lint:admin": "cd admin && pnpm run lint",
    "lint:client": "cd client && pnpm run lint"
  },
  "keywords": [
    "monorepo",
    "nextjs",
    "admin",
    "client"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "tailwindcss": "^4.1.18"
  }
}